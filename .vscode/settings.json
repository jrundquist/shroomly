{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "C_Cpp.errorSquiggles": "Disabled",
  "files.associations": {
    "*.tcc": "cpp",
    "fstream": "cpp",
    "string": "cpp",
    "array": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "memory": "cpp",
    "optional": "cpp",
    "string_view": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "vector": "cpp",
    "sstream": "cpp",
    "streambuf": "cpp",
    "functional": "cpp",
    "tuple": "cpp"
  }
}